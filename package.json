    "remark-emoji": "^3.0.2",
    "remark-external-links": "^9.0.1",
    "remark-gfm": "^3.0.1",
    "remark-hint": "^1.0.10",
    "remark-mark-plus": "^1.0.21",
    "remark-slug": "^7.0.1",
    "remark-sub-super": "^1.0.20",
    "tinacms": "^1.1.0"
