    "classnames": "^2.3.2",
    "fast-deep-equal": "^3.1.3",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "markdown-toc": "^1.2.0",
    "mdast-util-to-string": "^3.1.0",
    "mdx-embed": "^1.1.2",
    "next": "^13.0.5",
    "next-compose-plugins": "^2.2.1",
    "next-mdx-remote": "^4.0.3",
    "next-remote-watch": "2.0.0",
    "next-seo": "^5.5.0",
    "next-sitemap": "^3.1.25",
    "prettier": "^2.7.1",
